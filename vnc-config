# vncserver -SecurityTypes=VeNCrypt,TLSVnc -geometry 2080x1080 -depth 24 :1
  # - Starts VNC using TLS encryption


# xstartup file located in $HOME/.vnc/
#!/bin/sh
def
export XKL_XMODMAP_DISABLE=1
unset SESSION_MANAGER
unset DBUS_SESSION_BUS_ADDRESS

xrdb $HOME/.Xresources
xsetroot -solid grey
startxfce4 &
